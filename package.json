{
  "name": "json-viewer",
  "version": "2.0.0",
  "description": "A javascript library to visualize json nicely",
  "main": "dist/iife/index.js",
  "module": "dist/esm/index.mjs",
  "keywords": [
    "json",
    "browser",
    "pretty",
    "react"
  ],
  "scripts": {
    "test": "jest",
    "build": "esbuild src/index.js --outdir=dist --bundle --minify --format=esm --target=es6 --loader:.css=text",
    "start": "esbuild src/index.js --outfile=public/dist/index.js --loader:.css=text --bundle --sourcemap --watch --servedir=public --serve=0.0.0.0:3000"
  },
  "author": "Andreas Pfau",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.17.18",
    "jest": "^29.7.0"
  }
}
